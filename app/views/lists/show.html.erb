<div class="container">

    <a href="/songs">Dashboard  |</a>
    <a href="/sessions/logout"> LogOut</a>

    <div class="col-sm-5">
        <h1><%= @user.first_name %> <%= @user.last_name %> Playlist</h1>
        <table class="table">
            <thead class="thead-light">
                <th>Artist</th>
                <th>Title</th>
                <th>Times Added</th>
            </thead>
            <tbody>
                <% @songs.each do |song| %>
                <tr>
                    <td><%= song.song.artist%></td>
                    <td><%= song.song.title %></td>
                    <td><%= song.count %></td>
                </tr>
                <% end %>
            </tbody>
        </table>
    </div>

</div>
